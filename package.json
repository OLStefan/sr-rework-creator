{
	"name": "sr-rework-creator",
	"homepage": "http://olstefan.github.io/sr-rework-creator",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"develop": "yarn install && parcel src/index.html -p 3000",
		"build": "parcel build src/index.html --public-url .",
		"build:docker": "docker build . -t sr-rework-creator",
		"clean": "rm -rf dist node_modules .parcel-cache",
		"test": "jest --",
		"test:watch": "yarn test --watch",
		"check:types": "tsc --noEmit",
		"check:style": "prettier --check ./src",
		"check:lint": "eslint . --ext .tsx --ext .ts",
		"check:all": "yarn check:types && yarn check:style && yarn check:lint && yarn test",
		"fix:style": "prettier --write ./src"
	},
	"browserslist": "> 0.5%, last 2 versions, not dead",
	"dependencies": {
		"@reduxjs/toolkit": "~1.8.6",
		"file-saver": "^2.0.5",
		"framer-motion": "^7.6.1",
		"i18next": "^22.0.2",
		"lodash": "^4.17.21",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hooks": "^1.0.1",
		"react-i18next": "^12.0.0",
		"react-is": "^18.2.0",
		"react-redux": "^8.0.4",
		"react-sortablejs": "^6.1.4",
		"react-textarea-autosize": "^8.3.4",
		"redux": "^4.2.0",
		"redux-undo": "^1.0.1",
		"sortablejs": "^1.15.0",
		"state-array-functions": "^0.9.2",
		"styled-components": "^5.3.6",
		"uint8-to-base64": "^0.2.0",
		"use-updating-callbacks": "^1.0.1",
		"uuid": "^9.0.0"
	},
	"devDependencies": {
		"@parcel/packager-raw-url": "2.7.0",
		"@parcel/transformer-webmanifest": "2.7.0",
		"@types/file-saver": "^2.0.5",
		"@types/jest": "^29.2.0",
		"@types/lodash": "^4.14.186",
		"@types/node": "^16.18.0",
		"@types/react": "^18.0.22",
		"@types/react-dom": "^18.0.7",
		"@types/react-redux": "^7.1.24",
		"@types/resize-observer-browser": "^0.1.7",
		"@types/sortablejs": "^1.15.0",
		"@types/styled-components": "^5.1.26",
		"@types/uuid": "^8.3.4",
		"@typescript-eslint/eslint-plugin": "~5.39.0",
		"@typescript-eslint/parser": "~5.39.0",
		"eslint": "^8.26.0",
		"eslint-config-prettier": "~8.5.0",
		"eslint-import-resolver-typescript": "^3.5.2",
		"eslint-plugin-import": "~2.26.0",
		"eslint-plugin-jsx-a11y": "~6.6.1",
		"eslint-plugin-prettier": "~4.2.1",
		"eslint-plugin-react": "^7.31.10",
		"eslint-plugin-react-hooks": "^4.6.0",
		"parcel": "~2.7.0",
		"prettier": "^2.7.1",
		"process": "^0.11.10",
		"typescript": "^4.8.4"
	}
}
