{
	"name": "sr-rework-creator",
	"homepage": "http://olstefan.github.io/sr-rework-creator",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"develop": "yarn install && parcel src/index.html -p 3000",
		"build": "parcel build src/index.html --public-url .",
		"build:docker": "docker build . -t sr-rework-creator",
		"clean": "rm -rf dist node_modules .parcel-cache",
		"test": "jest --",
		"test:watch": "yarn test --watch",
		"check:types": "tsc --noEmit",
		"check:style": "prettier --check ./src",
		"check:lint": "eslint . --ext .tsx --ext .ts",
		"check:all": "yarn check:types && yarn check:style && yarn check:lint",
		"fix:style": "prettier --write ./src"
	},
	"browserslist": "> 0.5%, last 2 versions, not dead",
	"dependencies": {
		"@reduxjs/toolkit": "~1.9.1",
		"file-saver": "^2.0.5",
		"framer-motion": "^8.4.3",
		"i18next": "^22.4.9",
		"lodash": "^4.17.21",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-hooks": "^1.0.1",
		"react-i18next": "^12.1.4",
		"react-is": "^18.2.0",
		"react-redux": "^8.0.5",
		"react-sortablejs": "^6.1.4",
		"react-textarea-autosize": "^8.4.0",
		"redux": "^4.2.0",
		"redux-undo": "^1.0.1",
		"sortablejs": "^1.15.0",
