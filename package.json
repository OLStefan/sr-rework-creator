		"@parcel/transformer-webmanifest": "2.8.2",
		"@types/file-saver": "^2.0.5",
		"@types/jest": "^29.2.5",
		"@types/lodash": "^4.14.191",
		"@types/node": "^18.11.18",
		"@types/react": "^18.0.26",
		"@types/react-dom": "^18.0.10",
		"@types/react-redux": "^7.1.25",
		"@types/resize-observer-browser": "^0.1.7",
