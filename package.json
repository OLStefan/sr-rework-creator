{
	"name": "sr-rework-creator",
	"homepage": "http://olstefan.github.io/sr-rework-creator",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"develop": "yarn install && react-scripts start",
		"build": "react-scripts build",
		"clean": "rimraf node_modules build",
		"test": "react-scripts test --watchAll=false",
		"test:watch": "react-scripts test",
		"check:types": "tsc --noEmit",
		"check:style": "prettier --check .",
		"check:linter": "eslint . --ext .tsx --ext .ts",
		"check:all": "yarn check:types && yarn check:style && yarn check:linter && yarn test",
		"prettier:fix": "prettier --write .",
		"eject": "react-scripts eject",
		"predeploy": "yarn build",
		"deploy": "yarn lint && gh-pages -d build"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"file-saver": "^2.0.5",
		"framer-motion": "^4.0.0",
		"i18next": "^19.8.4",
		"lodash": "^4.17.20",
		"react": "^17.0.1",
		"react-dom": "^17.0.1",
		"react-hooks": "^1.0.1",
		"react-i18next": "^11.8.4",
		"react-is": "^17.0.1",
		"react-redux": "^7.2.2",
		"react-scripts": "^4.0.1",
		"react-sortablejs": "^6.0.0",
		"react-textarea-autosize": "^8.3.2",
		"redux": "^4.0.5",
		"redux-thunk": "^2.3.0",
		"redux-undo": "^1.0.1",
		"sortablejs": "^1.12.0",
		"state-array-functions": "^0.9.2",
		"styled-components": "^5.2.1",
		"uint8-to-base64": "^0.2.0",
		"use-updating-callbacks": "^0.9.5",
		"uuid": "^8.3.2"
	},
	"devDependencies": {
		"@types/file-saver": "^2.0.1",
		"@types/jest": "^26.0.19",
		"@types/lodash": "^4.14.165",
		"@types/node": "^14.14.14",
		"@types/react": "^17.0.3",
		"@types/react-dom": "^17.0.2",
		"@types/react-redux": "^7.1.12",
		"@types/resize-observer-browser": "^0.1.5",
		"@types/sortablejs": "^1.10.6",
		"@types/styled-components": "^5.1.7",
		"@types/uuid": "^8.3.0",
		"eslint": "^7.22.0",
		"eslint-config-prettier": "^8.1.0",
		"eslint-plugin-import": "^2.22.1",
		"eslint-plugin-jsx-a11y": "^6.4.1",
		"eslint-plugin-prettier": "^3.3.0",
		"eslint-plugin-react": "^7.21.5",
		"eslint-plugin-react-hooks": "^4.2.0",
		"gh-pages": "^3.1.0",
		"prettier": "^2.2.1",
		"redux-devtools-extension": "^2.13.8",
		"typescript": "^4.1.3"
	}
}
